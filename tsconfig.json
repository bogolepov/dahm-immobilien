{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@vue/*": ["src/components/vue/*"],
			"@components/*": ["src/components/*"],
			"@layouts/*": ["src/layouts/*"],
			"@styles/*": ["src/styles/*"],
			"@data/*": ["src/data/*"],
			"@icons/*": ["src/icons/*"],
			"@assets/*": ["src/assets/*"],
			"@scripts/*": ["src/scripts/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	}
}
